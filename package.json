{
  "name": "predict-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "bun prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "bun prisma generate",
    "prisma:studio": "bun prisma studio",
    "prisma:seed": "bun prisma db seed",
    "db:setup": "node scripts/setup-database.js",
    "db:reset": "npx prisma migrate reset --force",
    "db:deploy": "npx prisma migrate deploy",
    "deploy:testnet": "npx hardhat run scripts/deploy.js --network core_testnet",
    "deploy:mainnet": "npx hardhat run scripts/deploy.js --network core_mainnet",
    "create:markets": "npx hardhat run scripts/create-sample-markets.js",
    "test:blockchain": "npx hardhat run scripts/test-blockchain.js",
    "test:api": "npm run test -- __tests__/api",
    "test:components": "npm run test -- __tests__/components",
    "test": "jest",
    "test:watch": "jest --watch",
    "setup": "node scripts/full-setup.js",
    "setup:dev": "npm run db:setup && npm run deploy:testnet && npm run create:markets",
    "setup:prod": "NODE_ENV=production node scripts/full-setup.js",
    "verify:production": "npx hardhat run scripts/verify-production.js --network core_mainnet",
    "verify:testnet": "npx hardhat run scripts/verify-production.js --network core_testnet"
  },
